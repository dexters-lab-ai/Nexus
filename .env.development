# Server Configuration
NODE_ENV=development
PORT=3420  # Backend server port
DOCKER=false
HOST=localhost

# API Configuration
VITE_API_URL=http://localhost:3420  # Backend API URL for Vite
VITE_WS_URL=ws://localhost:3420      # WebSocket URL for Vite
API_URL=http://localhost:3420        # Backend API URL
FRONTEND_URL=http://localhost:3000   # Frontend development server

# Database
MONGO_URI="mongodb://localhost:27017/nexus"

# Session & Security
SESSION_SECRET=generate_a_secure_random_string_here
COOKIE_DOMAIN=localhost
SECURE_COOKIES=false
ALLOWED_CORS_ORIGINS=http://localhost:3000,http://localhost:3420

# CSP Configuration
CSP_FONTAWESOME_URL=https://cdnjs.cloudflare.com
CSP_GOOGLE_FONTS_URL=https://fonts.googleapis.com
CSP_GOOGLE_FONTS_CSS_URL=https://fonts.googleapis.com/css2

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY_FILE=config/your-google-key.json
HF_API_KEY=your_huggingface_api_key_here

# Default API Keys for LLM Engines
DEFAULT_GPT4O_KEY=your_gpt4o_key_here
DEFAULT_OPENROUTER_KEY=your_openrouter_key_here
DEFAULT_GEMINI_KEY=your_gemini_key_here
DEFAULT_UITARS_KEY=your_uitars_key_here

# Midscene Configuration
NEXUS_RUN_DIR=/usr/src/app/nexus_run
MIDSCENE_MODEL_NAME="gpt-4o-mini"
MIDSCENE_RUN_DIR="nexus_run"

# Webhook & WebSocket
WEBHOOK_URL=http://localhost:3000/api/webhook
WEBHOOK_SECRET=generate_a_secure_webhook_secret
OPERATOR_APP_WS_URL=ws://localhost:3000/ws
WS_SECRET=generate_a_secure_ws_secret

# Debugging
DEBUG_PUPPETEER_LOGS=false
REACT_APP_WS_SESSION_CHECK_INTERVAL=300000  # 5 minutes
