{
    "name": "operator-agent",
    "version": "1.3.9",
    "description": "O.P.E.R.A.T.O.R - Advanced autonomous agent with browser and computer use capabilities. Nexus Engine written in NodeJs. Product of D.A.I.L",
    "type": "module",
    "repository": {
        "type": "git",
        "url": "https://github.com/dexters-lab-ai/Nexus.git"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "prepare": "npx puppeteer browsers install chrome",
        "start": "cross-env NODE_ENV=production pm2 start ecosystem.config.yaml --no-daemon",
        "dev": "cross-env NODE_ENV=development concurrently --names \"VITE,SERVER\" --prefix-colors \"green,blue\" --raw \"vite\" \"node --max-old-space-size=4096 server.js\"",
        "build": "cross-env NODE_ENV=production vite build && npm run copy-assets",
        "preview": "cross-env NODE_ENV=production vite preview --port 3000",
        "serve": "cross-env NODE_ENV=production node --max-old-space-size=4096 server.js",
        "postinstall": "npx puppeteer browsers install chrome",
        "install:fontawesome": "npm install @fortawesome/fontawesome-free",
        "copy-assets": "node scripts/copy-assets.js",
        "lint": "eslint . --ext .js,.jsx --fix",
        "test": "vitest",
        "test:watch": "vitest watch"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.6.0",
        "@floating-ui/react": "^0.27.9",
        "@fortawesome/fontawesome-free": "^6.7.2",
        "@mantine/core": "^8.0.2",
        "@mantine/hooks": "^8.0.2",
        "@mantine/notifications": "^8.0.2",
        "@midscene/web": "^0.12.7",
        "@nut-tree-fork/nut-js": "4.2.6",
        "@playwright/test": "^1.51.1",
        "@puppeteer/browsers": "^2.10.5",
        "@react-three/drei": "10.0.6",
        "@react-three/fiber": "9.1.2",
        "@types/three": "^0.175.0",
        "async-mutex": "^0.5.0",
        "axios": "^1.9.0",
        "bcrypt": "^5.1.1",
        "cheerio": "^1.0.0",
        "connect-mongo": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "fs-extra": "^11.3.0",
        "gsap": "^3.12.7",
        "htmlparser2": "^10.0.0",
        "lil-gui": "^0.20.0",
        "lru-cache": "^10.2.0",
        "marked": "^15.0.8",
        "mongodb": "^6.16.0",
        "mongoose": "^6.13.8",
        "morgan": "^1.10.0",
        "openai": "^4.88.0",
        "p-retry": "^6.2.1",
        "p-timeout": "^6.1.2",
        "playwright": "^1.37.1",
        "puppeteer": "^24.4.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2",
        "rate-limiter-flexible": "^5.0.0",
        "three": "^0.175.0",
        "tweakpane": "^4.0.3",
        "winston": "^3.11.0",
        "ws": "^8.18.2",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "@types/react": "19.0.0",
        "@types/react-dom": "19.0.0",
        "@vitejs/plugin-react": "^4.3.2",
        "autoprefixer": "^10.4.21",
        "concurrently": "^9.1.2",
        "cross-env": "^7.0.3",
        "nodemon": "^3.1.9",
        "pm2": "^6.0.5",
        "postcss": "^8.5.3",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-router-dom": "6.22.3",
        "rollup-plugin-visualizer": "5.9.2",
        "terser": "^5.40.0",
        "typescript": "^5.8.3",
        "vite": "^6.3.2"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "latest"
    }
}